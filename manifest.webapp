{
  "name": "Ripcord",
  "version": "1.0",
  "description": "The ripcord error reporting app",
  "launch_path": "/index.html",
  "appcache_path": "/ripcord/manifest.appcache",
  "icons": { },
  "developer": {
    "name": "Lonnen",
    "url": "http://xor.lonnen.com/"
  },
  "activities": {
    "error": {
        "filters": {
          type: ["application/json"],
        },
        "href": "index.html",
        "disposition": "inline"
      }
  },
  "default_locale": "en"
}
